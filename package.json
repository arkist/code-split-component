{
  "name": "code-split-component",
  "version": "0.0.3",
  "description": "A React Component and Babel 6 plugin to support code splitting in Webpack 2 powered projects.",
  "main": "lib/index.js",
  "engines": {
    "node": "~6.7.0"
  },
  "scripts": {
    "build": "babel src -d lib --presets latest,react",
    "prepublish": "npm run build",
    "example:prod": "NODE_ENV=production rimraf ./example/build && webpack --config ./example/webpack.config.js && node ./example/server.js",
    "example:dev": "NODE_ENV=development node ./example/devServer.js"
  },
  "files": [
    "lib",
    "babel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrlplusb/code-split-component.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "babel plugin",
    "code splitting"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ctrlplusb/code-split-component/issues"
  },
  "homepage": "https://github.com/ctrlplusb/code-split-component#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "express": "4.14.0",
    "react": "^15.3.2",
    "react-dom": "15.3.2",
    "react-hot-loader": "3.0.0-beta.5",
    "react-router": "4.0.0-alpha.4",
    "rimraf": "2.5.4",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "babel-template": "6.16.0"
  },
  "dependencies": {},
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        0
      ],
      "react/jsx-filename-extension": [
        0
      ],
      "global-require": [
        0
      ]
    }
  }
}
